{
  "name": "Dominion Helper",
  "version": "1.0.0",
  "description": "Become a dominion master",
  "manifest_version": 3,
  "author": "Sam Cohen",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["activeTab", "scripting"]
}
